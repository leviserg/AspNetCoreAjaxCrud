<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AspNetCoreAjaxModal</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/font-awesome.css" rel="stylesheet" />
    <!--<link href="~/lib/bootstrap/dist/css/bootstrap-theme.css" rel="stylesheet" /> -->
    <link href="~/lib/bootstrap/dist/css/bootstrap_glyph_icons_only.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-theme_glyphicons.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/response.js"></script>
</head>
<body>
    <nav class="navbar navbar-expand-md bg-info navbar-dark">
        <a asp-area="" asp-controller="Transaction" asp-action="Index" class="navbar-brand" data-toggle="tooltip" title="Transactions"><i class="fa fa-home mx-2"></i></a>
        <button class="navbar-toggler navbar-toggler-right" type="button"
                data-toggle="collapse" data-target="#navbarResponsive"
                aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            <i class="fa fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse justify-content-between" id="navbarResponsive">
            <ul class="navbar-nav">
                <!--
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="cursor:pointer">
                            <i class="fa fa-cubes mx-2"></i><span id="pagetitle">Участки</span>
                        </a>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                            <a class="dropdown-item text-info" href=""><i class="fa fa-cube mx-2"></i>DropDownLink1</a>
                            <a class="dropdown-item text-info" href=""><i class="fa fa-cube mx-2"></i>DropDownLink2</a>
                        </div>
                    </li>
                -->
                <li class="nav-item">
                    <a asp-area="" asp-controller="Transaction" asp-action="Index" class="nav-link mx-2 px-2" data-toggle="tooltip" title="Transactions"><i class="fa fa-files-o mx-2"></i>Transactions</a>
                </li>
                <li class="nav-item">
                    <a asp-area="" asp-controller="Bank" asp-action="Index" class="nav-link mx-2 px-2" data-toggle="tooltip" title="Banks"><i class="fa fa-tasks mx-2"></i>Banks</a>
                </li>
            </ul>
        </div>
    </nav>

    <partial name="_CookieConsentPartial" />

    <div class="container body-content">
        @RenderBody()
        <hr />

        <div class="mt-5 modal fade modal-fade" id="form-modal" tabindex="2">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header mb-0 mt-0">
                        <h5 class="modal-title"></h5>
                        <button class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                    </div>
                    <div class="modal-body pb-0 mb-0 px-3">
                    </div>
                </div>
            </div>
        </div>
        <a href="#" class="scrollup"></a>
    </div>

    <div class="loaderbody" id="loaderbody">
        <div class="loader"> </div>
    </div>

    <div class="bg-info almstrip">
        <p>
            &copy; 2021
            <a asp-area="" asp-controller="Transaction" asp-action="Index" class="pl-2" style="color:#E3F2F2">
                AspNetCoreAjaxModal
            </a>
            <span class="text-white-50 text-right" id="contactlink" style="position:fixed; right:1%"><small>&copy; Tech Support</small></span>
        </p>
    </div>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery/dist/sweetalert.js"></script>
    <script src="~/lib/jquery/dist/notify.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.xdomainrequest.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/lib/jquery/dist/popper.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/moment/moment.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap-datetimepicker.js"></script>
    <script src="~/js/site.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
